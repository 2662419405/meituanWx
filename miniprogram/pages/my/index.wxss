.head_top {
  background-color: #3190e8;
  position: fixed;
  z-index: 100;
  left: 0;
  top: 0;
  width: 100%;
  height: 2.5rem;
}

.title_head {
  position: absolute;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  width: 50%;
  color: #fff;
  text-align: center;
}

.title_text {
  font-size: 1.1rem;
  color: #fff;
  text-align: center;
  font-weight: 700;
}

.profile-number {
  padding-top: 2.5rem;
}

.xiugai {
  background: #3190e8;
  border: none;
  outline: none;
}

.tiaozhuan {
  display: block;
  display: -ms-flexbox;
  display: flex;
  box-align: center;
  -ms-flex-align: center;
  align-items: center;
  background: #3190e8;
  padding: 1rem;
  color: #fff;
}

.privateImage {
  display: inline-block;
  width: 3rem;
  height: 3rem;
  border-radius: 50%;
  vertical-align: middle;
}

.privateImage image {
  background: #fff;
  border-radius: 50%;
  width: 3rem;
  height: 3rem;
}

.arrow {
  width: 0.6rem;
  height: 1.2rem;
  display: inline-block;
  font-weight: bold;
}

.user-info {
  margin-left: 0.65rem;
  -ms-flex-positive: 1;
  flex-grow: 1;
}

.xiugai {
  color: #fff;
  margin-left: 0;
  margin: 0 !important;
  width: auto;
  text-align: left;
  padding: 8px 5px;
  font-size: 1.2rem;
  font-weight: bold;
}

.bianda {
  font-size: 1rem;
}

.info-data {
  width: 100%;
  background: #fff;
  box-sizing: border-box;
}

.clear {
  overflow: hidden;
}

.info-data-link {
  float: left;
  width: 33.33%;
  display: inline-block;
  border-right: 1px solid #f1f1f1;
  box-sizing: border-box;
  text-align: center;
}

.info-data-top {
  font-size: 0.7rem;
  color: #333;
  padding: 0.853333rem 0 0.453333rem;
}

.info-data-top text {
  display: inline-block;
  font-size: 1.7rem;
  font-weight: 700;
  line-height: 1.3rem;
  font-family: Helvetica Neue, Tahoma;
}

.info-data-top .b {
  color: #f90;
}

.info-data-top .c {
  color: #ff5f3e;
}

.info-data-top .d {
  color: #6ac20b;
}

.profile-1reTe {
  background: #fff;
}

.myorder {
  padding-left: 2.2rem;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
}

.aside {
  width: 1rem;
  height: 1rem;
  margin-left: -1rem;
  margin-right: 0.4rem;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-align: center;
  align-items: center;
}

.aside icon {
  position: relative;
  top: -0.25rem;
}

.jifen {
  color: rgb(252, 123, 83);
}

.huangguan {
  color: rgb(255, 198, 54);
}

.fuwuzhongxin {
  color: rgb(74, 165, 240);
}

.download {
  color: rgb(60, 171, 255);
}

.myorder-div {
  width: 100%;
  border-bottom: 1px solid #f1f1f1;
  padding: 0.65rem 0.4rem 0.65rem 0;
  font-size: 1rem;
  color: #333;
  display: -ms-flexbox;
  display: flex;
  -ms-flex-pack: justify;
  justify-content: space-between;
  box-sizing: border-box;
}

.myorder-divsvg {
  color: #666;
}

.info-data-bottom {
  font-size: 0.8rem;
  color: #666;
  font-weight: 400;
  padding-bottom: 0.7rem;
}

.rongqi_container {
  height: 100%;
  background: #f5f5f5;
}
