page {
  font-size: 30px;
}

.kong {
  background-color: #f5f5f5;
  height: 10px;
}

.ellipsis {
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 0.8rem;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.container {
  display: flex;
  flex-direction: column;
  box-sizing: border-box;
  padding: 8px;
}

@font-face {
  font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_1677276_ecmyfl5ooqu.eot?t=1583982587827'); /* IE9 */
  src: url('//at.alicdn.com/t/font_1677276_ecmyfl5ooqu.eot?t=1583982587827#iefix') format('embedded-opentype'),
         /* IE6-IE8 */ url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
         url('//at.alicdn.com/t/font_1677276_ecmyfl5ooqu.woff?t=1583982587827') format('woff'),
         url('//at.alicdn.com/t/font_1677276_ecmyfl5ooqu.ttf?t=1583982587827') format('truetype'),
         /* chrome,
         firefox,
         opera,
         Safari,
         Android,
         iOS 4.2+ */ url('//at.alicdn.com/t/font_1677276_ecmyfl5ooqu.svg?t=1583982587827#iconfont') format('svg'); /* iOS 4.1- */
